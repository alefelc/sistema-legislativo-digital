#otra_norma
  #otra_norma-edit
  %button#btn-new.btn.btn-primary
    %span.glyphicon.glyphicon-plus-sign{ "aria-hidden": true }
    = t('.btnheader.new')

  %table.datatable-keyevents.table.table-striped.table-bordered#otra_norma-table{"data-source" => otra_normas_path(format: :json)}
    %thead
      %tr
        %th= t('.table.otra_norma')
        %th= t('.table.sumario')
        %th{width: "15%"}= t('.table.fechas')
        %th= t('.table.expedientes')
        %th
    %tbody
